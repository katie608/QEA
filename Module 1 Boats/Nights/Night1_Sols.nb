Cell[TextData[{
 "\n",
 Cell[BoxData[
  FormBox[GridBox[{
     {GridBox[{
        {
         RowBox[{
          RowBox[{"x", " ", "=", " ", 
           RowBox[{"a", " ", 
            RowBox[{"cos", "(", "u", ")"}]}]}], ",", " ", 
          RowBox[{"y", " ", "=", " ", 
           RowBox[{"a", " ", 
