Cell[TextData[{
 StyleBox["For all ",
  FontSize->16],
 StyleBox["n ",
  FontSize->16,
  FontSlant->"Italic"],
 StyleBox["the function is a curve from (0,0) to (1,1). For ",
  FontSize->16],
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "=", "1"}], TraditionalForm]],
  FormatType->"TraditionalForm",
  FontSize->16,ExpressionUUID->"36e71791-8f23-422e-b2e0-43888a1bcdaf"],
 StyleBox["the curve is a straight-line. As ",
  FontSize->16],
 StyleBox["n",
  FontSize->16,
  FontSlant->"Italic"],
 StyleBox[" increases, the curve becomes flatter initially, but still rises \
to the point (1,1). As ",
  FontSize->16],
 StyleBox["n",
  FontSize->16,
  FontSlant->"Italic"],
 StyleBox[" approaches infinity, the curve tends becomes ",
  FontSize->16],
 StyleBox["0 ",
  FontSize->16,
  FontSlant->"Italic"],
 StyleBox["everywhere but jumps to (1,1) at the end - it looks like the side \
of a square.",
  FontSize->16]
}], "Text",
