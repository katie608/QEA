          RowBox[{"z", " ", "=", " ", 
           RowBox[{"b", " ", "u"}]}], ",", " ", 
          RowBox[{"a", " ", ">", " ", "0"}], ",", " ", 
          RowBox[{"b", " ", ">", " ", "0"}], ",", " ", 
          RowBox[{"u", " ", ">", " ", "0"}]}]}
       },
       GridBoxAlignment->{"Columns" -> {{"Center"}}}]}
