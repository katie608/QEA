Cell[TextData[{
 "Use ",
 StyleBox["ContourPlot",
  FontWeight->"Bold"],
 " and ",
 StyleBox["Manipulate",
  FontWeight->"Bold"],
 " to visualize the implicit equation for an hyperbola ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["x", "2"], 
      SuperscriptBox["a", "2"]], "-", 
     FractionBox[
      SuperscriptBox["y", "2"], 
      SuperscriptBox["b", "2"]], "-", "1"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"228e767a-5de9-4a94-b57d-a33c1e41eb94"],
