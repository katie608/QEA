Cell[TextData[{
 "Use ",
 StyleBox["ContourPlot",
  FontWeight->"Bold"],
 " and ",
 StyleBox["Manipulate",
  FontWeight->"Bold"],
 " to visualize the implicit equation for an ellipse ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     FractionBox[
      SuperscriptBox["x", "2"], 
      SuperscriptBox["a", "2"]], "+", 
     FractionBox[
      SuperscriptBox["y", "2"], 
      SuperscriptBox["b", "2"]], "-", "1"}], "=", "0"}], TraditionalForm]],
  ExpressionUUID->"f2c45340-636d-4331-9efe-b80fc1d4e9dc"],
