    RowBox[{
     RowBox[{"x", "\[Rule]", 
      FractionBox[
       RowBox[{
        RowBox[{
         RowBox[{"-", "b"}], " ", "m"}], "-", 
        SqrtBox[
         RowBox[{
          RowBox[{"-", 
           SuperscriptBox["b", "2"]}], "+", 
          SuperscriptBox["R", "2"], "+", 
          RowBox[{
           SuperscriptBox["m", "2"], " ", 
